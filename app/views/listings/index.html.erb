<p id="notice"><%= notice %></p>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Year</th>
      <th>Make</th>
      <th>Model</th>
      <th>Body Style</th>
      <th>Mileage</th>
      <th>Transmission</th>
      <th>Condition</th>
      <th>Fuel Economy</th>
      <th>Engine Size</th>
      <th>Cylinders</th>
      <th>Power</th>
      <th>Fuel Type</th>
      <th>Vin</th>
      <th>Overview</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.year %></td>
        <td><%= listing.make %></td>
        <td><%= listing.model %></td>
        <td><%= listing.body_style %></td>
        <td><%= listing.mileage %></td>
        <td><%= listing.transmission %></td>
        <td><%= listing.condition %></td>
        <td><%= listing.fuel_economy %></td>
        <td><%= listing.engine_size %></td>
        <td><%= listing.cylinders %></td>
        <td><%= listing.power %></td>
        <td><%= listing.fuel_type %></td>
        <td><%= listing.vin %></td>
        <td><%= listing.overview %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
